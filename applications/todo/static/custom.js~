var initialize() = function() {
    var self  = {}; 
    self.data = {);
    self.methods = {};
    self.filters = {};
    self.start = function() {
        self.vue = new Vue({el: '#vue', data: self.data, methods: self.methods, filters: self.filters});        
    };
    return self;
};    

var myapp = initialize();
myapp.start();